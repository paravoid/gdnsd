
options => {
  listen => @dns_lspec@
  http_listen => @http_lspec@
  dns_port => @dns_port@
  http_port => @http_port@
  plugin_search_path = @pluginpath@
  realtime_stats = true
}

service_types => {
    www_extraport => {
        plugin => tcp_connect
        port = @extra_port@
        up_thresh = 15
        timeout = 1
    }
}

plugins => {
  simplefo => {
    dyn_xmpl => {
      service_types = [ www_extraport, www_extraport ]
      primary = 127.0.0.1
      secondary = 192.0.2.1
    }
  }
  multifo => {
    multi_xmpl => {
      service_types = www_extraport
      addrs_v4 => {
        pri = 127.0.0.1
        sec = 192.0.2.1
      }
    }
  }
}
